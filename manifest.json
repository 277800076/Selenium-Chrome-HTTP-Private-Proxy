{
    "name": "Proxy Helper",
    "version": "1.1.4",
    "manifest_version": 2,
    "description": "Set proxy for Google Chrome browser",
    "icons": {
        "128": "images/on.png",
        "48":  "images/on48.png",
        "16":  "images/on16.png"
    },
    "permissions": [
        "proxy",
        "tabs",
        "unlimitedStorage"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "images/off.png",
        "default_title": "Proxy Helper",
        "default_popup": "popup.html"
    }
}
